<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Waterfox</ShortName>
<Description>Waterfox Search</Description>
<InputEncoding>UTF-8</InputEncoding>
<Image width="16" height="16">data:image/png;base64,</Image>
<Url type="application/x-suggestions+json" method="GET"
     template="https://search.waterfoxproject.org/getSearchBarSuggestions?term={searchTerms}" />
<Url type="text/html" method="GET" template="https://search.waterfoxproject.org/">
  <Param name="q" value="{searchTerms}"/>
</Url>
<SearchForm>https://search.waterfoxproject.org/</SearchForm>
</SearchPlugin>