config-lock-column =
    .label = Stanje
config-type-column =
    .label = Vrsta
config-value-column =
    .label = Vrijednost

## These strings are used for tooltips

config-pref-column-header =
    .tooltip = Pritisni za poredak
config-column-chooser =
    .tooltip = Kliknite za odabir stupaca koji će biti prikazani

