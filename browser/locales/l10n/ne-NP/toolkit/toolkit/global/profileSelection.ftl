    .title = { -brand-short-name }-प्रयोगकर्ता को खाता छान्नुहोस्

profile-selection-button-accept =
    .label = { -brand-short-name } सुरू गर्नुहोस

profile-selection-button-cancel =
    .label = निस्कनुहोस्

