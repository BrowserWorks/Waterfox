        { $tabCount ->
            [1] Začinjeny rajtark wočinić
            [one] Začinjeny rajtark wočinić
            [two] Začinjenej rajtarkaj wočinić
            [few] Začinjene rajtarki wočinić
           *[other] Začinjene rajtarki wočinić
        }
    .accesskey = Z

