profiledowngrade-window =
    .title = הפעלת גרסה ישנה של { -brand-product-name }
    .style = width: 490px;

profiledowngrade-window-create =
    .label = יצירת פרופיל חדש

profiledowngrade-sync = שימוש בגרסה ישנה של { -brand-product-name } עלול לפגוע בסימניות ובהיסטוריית הגלישה שכבר שמורים בפרופיל קיים ב־{ -brand-product-name }. כדי להגן על המידע שלך, יש ליצור פרופיל חדש עבור התקנה זו של { -brand-short-name }. תמיד ניתן להיכנס באמצעות { -fxaccount-brand-name } כדי לסנכרן את הסימניות והיסטוריית הגלישה שלך בין פרופילים.
profiledowngrade-nosync = שימוש בגרסה ישנה של { -brand-product-name } עלול לפגוע בסימניות ובהיסטוריית הגלישה שכבר שמורים בפרופיל קיים ב־{ -brand-product-name }. כדי להגן על המידע שלך, יש ליצור פרופיל חדש עבור התקנה זו של { -brand-short-name }.

profiledowngrade-quit =
    .label =
        { PLATFORM() ->
            [windows] יציאה
           *[other] יציאה
        }
