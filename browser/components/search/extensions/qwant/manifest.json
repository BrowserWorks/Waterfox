{
  "name": "Qwant",
  "description": "Search Qwant",
  "manifest_version": 2,
  "version": "1.0",
  "applications": {
    "gecko": {
      "id": "qwant@search.waterfox.net"
    }
  },
  "hidden": true,
  "icons": {
    "16": "favicon.ico"
  },
  "web_accessible_resources": [
    "favicon.ico"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Qwant",
      "search_url": "https://www.qwant.com/",
      "search_form": "https://www.qwant.com/?q={searchTerms}",
      "search_url_get_params": "q={searchTerms}",
      "suggest_url": "https://api.qwant.com/api/suggest/",
      "suggest_url_get_params": "client=opensearch&q={searchTerms}"
    }
  }
}