{
  "name": "Startpage",
  "description": "Startpage.com Search",
  "manifest_version": 2,
  "version": "1.0",
  "applications": {
    "gecko": {
      "id": "startpage@search.waterfox.net"
    }
  },
  "hidden": true,
  "icons": {
    "16": "favicon.ico"
  },
  "web_accessible_resources": [
    "favicon.ico"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Startpage",
      "search_url": "https://www.startpage.com/do/dsearch",
      "search_form": "https://www.startpage.com/do/dsearch?query={searchTerms}&segment=startpage.waterfox.1",
      "search_url_post_params": "query={searchTerms}&segment=startpage.waterfox.1",
      "params": [
      ],
      "suggest_url": "https://www.startpage.com/cgi-bin/csuggest",
      "suggest_url_post_params": "query={searchTerms}&limit=10&lang=english&format=json"
    }
  }
}
