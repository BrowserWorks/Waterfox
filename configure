     Darwin|WINNT|Linux)
       MOZ_GAMEPAD=1
       ;;
     Android)
       if test "$MOZ_WIDGET_TOOLKIT" != "gonk"; then
         MOZ_GAMEPAD=1
       fi
       ;;
     *)
       ;;
esac

# Check whether --enable-gamepad or --disable-gamepad was given.
